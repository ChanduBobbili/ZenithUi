{
  "name": "@zenithui/primitive",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "pnpm": ">=9",
    "node": ">=20"
  },
  "packageManager": "pnpm@9.5.0",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "vite build && tsc",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "lint:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --max-warnings 0 --fix",
    "format": "prettier --write ."
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^17.0.0",
    "react-dom": "^18.0.0 || ^17.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-config-custom": "workspace:*",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.3",
    "tsconfig": "workspace:*",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-lib-inject-css": "^2.2.1"
  }
}
