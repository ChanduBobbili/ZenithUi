{
  "name": "@zenithui/utils",
  "version": "0.0.5",
  "description": "The utils package which provides the utils.",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rslib build",
    "dev": "rslib build --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@rsbuild/plugin-react": "^1.1.1",
    "@rslib/core": "^0.5.5",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.2",
    "rsbuild-plugin-dts": "^0.5.5",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.25.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "engines": {
    "pnpm": ">=9",
    "node": ">=20"
  },
  "homepage": "https://zenithui-docs.vercel.app/",
  "bugs": {
    "url": "https://github.com/ChanduBobbili/ZenithUi/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChanduBobbili/ZenithUi.git#main"
  },
  "license": "MIT",
  "author": "Chandu Bobbili <chandubobbili12@gmail.com> (https://github.com/ChanduBobbili)",
  "packageManager": "pnpm@9.5.0"
}
