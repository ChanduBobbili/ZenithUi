{
  "name": "@zenithui/utils",
  "version": "0.0.1",
  "description": "The utils package which provides the utils.",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rslib build",
    "dev": "rslib build --watch",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@rsbuild/plugin-react": "^1.1.1",
    "@rslib/core": "^0.5.5",
    "@types/react": "^19.0.12",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.2",
    "react": "^19.0.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.25.0"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  }
}
