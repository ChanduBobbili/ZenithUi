{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**", "out/**", "public/**"]
    },
    "clean": {},
    "lint": {},
    "lint:fix": {},
    "next:clean": {},
    "start": {
      "cache": false,
      "persistent": true
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "storybook": {
      "cache": false,
      "persistent": true
    }
  }
}
