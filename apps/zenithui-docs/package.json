    "@radix-ui/react-toggle-group": "^1.1.2",
    "@vercel/speed-insights": "^1.2.0",
    "@zenithui/day-picker": "workspace:*",
    "@zenithui/time-picker": "workspace:*",
    "@zenithui/toast": "workspace:*",
    "@zenithui/utils": "workspace:*",
    "class-variance-authority": "^0.7.1",
